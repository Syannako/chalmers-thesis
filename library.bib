month = nov,
number = {44},
pages = {14651--8},
pmid = {18847262},
title = {{DNA polymorphism as an origin of adenine-thymine tract length-dependent threading intercalation rate.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18847262},
volume = {130},
year = {2008}
