title = {{Comparison of Boolean Satisfiability Encodings on FPGA Detailed Routing Problems}},
type = {Conference proceedings (whole)},
url = {http://ieeexplore.ieee.org/xpls/abs_all.jsp?arnumber=4484853},
year = {2008}
}
