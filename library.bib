@inproceedings{Astesana2010a,
abstract = {As outlined by [10] (Junker and Mailharro 2003, ILOG jConfigurator) ”constraint programming is well- suited to treat complex configuration constraints such as compatibility and requirement- constraints in addition to standard arithmetic, comparison, and boolean constraints”. As a consequence several configurators have been implemented that are based on this technology. Nevertheless, configuration applications are not be restricted to the sole problem of on-line product configuration. Different business tasks may occur at different steps of the life cycle of the product range, e.g. the design of the model, the management of the Bill of material, etc. In the present paper, we analyse and formalize a series of the business needs that arise around the problem of car configuration at Renault’s, the French car manufacturer. It appears that these tasks call for a series of requests to a constraint satisfaction problem (CSP) that often differ from the ones classically handeled by the contraint programming frameworks. This application actually appears as a fruitful source of research problematics for both the CSP community and the Configuration community. We aim at motivating this research, by a formalization and a theoretical study, in terms on complexity, of the series of requests risen by this application.},
author = {Astesana, Jean-Marc and Bossu, Yves and Cosserat, Laurent and Fargier, H\'{e}l\`{e}ne},
booktitle = {ECAI 2010 Workshop on Configuration},
file = {:Users/alex/Documents/Mendeley Desktop/Astesana et al.. 2010. Constraint-based Modeling and Exploitation of a Vehicle Range at Renault’s Requirement analysis and complexity study.pdf:pdf},
pages = {33--39},
title = {{Constraint-based Modeling and Exploitation of a Vehicle Range at Renault’s: Requirement analysis and complexity study}},
url = {http://www.hitec-hh.de/confws10/Proceedings-Configuration-WS-2010.pdf ftp://ftp.irit.fr/IRIT/ADRIA/PapersFargier/ECAIWS10.pdf},
year = {2010}
}
